{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss}  andyx-schemahub   {Level,-12} {0,-3} | {0,-3} {Message}{NewLine}{Exception}"
        }
      }
    ]
  },

  "NodeId": "andyxschemahub_standalone",

  "Credentials": [
    {
      "Username": "admin",
      "Password": "admin",
      "Role": "admin"
    },
    {
      "Username": "readonly",
      "Password": "readonly",
      "Role": "readonly"
    }
  ],

  "AllowedHosts": "*"
}
